{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "model": "deepseek-chat",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 30
    },
    "named": {
      "econ_watcher": {
        "workspace": "~/.picoclaw/workspace_econ",
        "model": "deepseek-chat",
        "max_tokens": 8192,
        "temperature": 0.7,
        "max_tool_iterations": 30
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "YOUR_TELEGRAM_BOT_TOKEN",
      "allow_from": [
        "YOUR_USER_CHAT_ID"
      ]
    },
    "discord": {
      "enabled": false,
      "token": "YOUR_DISCORD_BOT_TOKEN",
      "allow_from": []
    }
  },
  "providers": {
    "deepseek": {
      "api_key": "YOUR_DEEPSEEK_API_KEY",
      "api_base": "https://api.deepseek.com"
    },
    "openrouter": {
      "api_key": "sk-or-v1-xxx",
      "api_base": ""
    }
  },
  "tools": {
    "web": {
      "search": {
        "api_key": "YOUR_BRAVE_API_KEY",
        "max_results": 5
      }
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}